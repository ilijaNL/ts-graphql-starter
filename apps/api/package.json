{
  "name": "api",
  "author": "IlijaNL",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "tc": "tsc --noEmit",
    "lint": "eslint src",
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "format": "prettier --write src",
    "build": "rimraf dist && tsc",
    "start": "node -r ./bootstrap.js dist/server.js",
    "gen:auth": "kysely-codegen --include-pattern \"(auth).*\" --schema auth --url postgres://postgres:postgres@localhost:5432/app --out-file ./src/services/auth/__generated__/auth-db.d.ts",
    "gen": "npm-run-all gen:*",
    "test": "tap --node-arg=--require=tsconfig-paths/register --ts --no-check-coverage",
    "test:cov": "tap --coverage-report=html --no-check-coverage",
    "test:watch": "tap -w --node-arg=--require=tsconfig-paths/register --ts"
  },
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^10.1.1",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/type-provider-typebox": "^3.2.0",
    "@sinclair/typebox": "^0.28.14",
    "@ts-hasura-starter/api": "workspace:*",
    "@ts-hasura-starter/rpc": "workspace:*",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "async-cache-dedupe": "^1.12.0",
    "date-fns": "^2.30.0",
    "delay": "^5.0.0",
    "dotenv": "^16.1.1",
    "fast-jwt": "^2.2.3",
    "fastify": "^4.17.0",
    "fastify-plugin": "^4.5.0",
    "grant": "^5.4.21",
    "http-errors": "^2.0.0",
    "kysely": "^0.24.2",
    "lodash": "^4.17.21",
    "node-batcher": "^0.1.3",
    "nodemailer": "^6.9.3",
    "pg": "^8.11.0",
    "pg-promise": "^11.5.0",
    "pg-tbus": "^0.1.9",
    "safe-stable-stringify": "^2.4.3",
    "ts-pattern": "^4.3.0",
    "tsconfig-paths": "^4.2.0",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^16.18.34",
    "@types/pg": "^8.10.1",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "close-with-grace": "^1.2.0",
    "eslint": "8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "kysely-codegen": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsx": "^3.12.7",
    "typescript": "^4.9.5"
  }
}
