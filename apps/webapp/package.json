{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:app": "react-env --prefix NEXT_APP -- next -p 4000",
    "dev": "concurrently \"pnpm dev:app\" \"graphql-codegen --watch\"",
    "build": "react-env --prefix NEXT_APP -- next build",
    "start": "react-env --prefix NEXT_APP -- next start",
    "tc": "tsc --noEmit",
    "lint": "eslint src",
    "gen": "graphql-codegen",
    "gen:watch": "graphql-codegen --watch"
  },
  "dependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mantine/core": "^6.0.7",
    "@mantine/ds": "^6.0.7",
    "@mantine/hooks": "^6.0.7",
    "@mantine/modals": "^6.0.7",
    "@mantine/next": "^6.0.7",
    "@mantine/notifications": "^6.0.7",
    "@tanstack/react-query": "^4.28.0",
    "cookie": "^0.5.0",
    "date-fns": "^2.25.0",
    "dotenv-safe": "^8.2.0",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "safe-stable-stringify": "^2.4.3",
    "ts-pattern": "^4.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/client-preset": "^3.0.0",
    "@next/bundle-analyzer": "^13.2.4",
    "@types/cookie": "^0.5.1",
    "@types/node": "^16.0.0",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.0.11",
    "concurrently": "^8.0.1",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "graphql-operation-list": "^0.0.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.0"
  }
}
